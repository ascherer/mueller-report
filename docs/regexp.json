{
    "template": {
        "match": null,
        "replace": null
    },
    "expand footnotes": {
        "match": "% (\d+)$",
        "replace": "% $1\n\\footnote{$1}"
    },
    "transform section glyph": {
        "match": "§",
        "replace": "\\S"
    },
    "apostrophe 1": {
        "match": "’",
        "replace": "'"
    },
    "quote 1": {
        "match": "“",
        "replace": "``"
    },
    "quote 2": {
        "match": "”",
        "replace": "''"
    },

}
